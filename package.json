{
  "name": "album-api",
  "version": "1.0.0",
  "description": "AWS album API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src\"",
    "build": "rimraf dist && npx babel src -D -d dist",
    "start": "rimraf dist && npx babel src -D -d dist --no-comments && node dist/index.js",
    "clean": "rimraf dist colony node_modules",
    "colony": "rimraf colony && npx babel src -D -d dist && npx colony dist/index.js --scale 4 --no-modules",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "lint": "eslint src",
    "test": "jest"
  },
  "engines": {
    "node": "10.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MindeVieras/album-api.git"
  },
  "author": "Minde Vieras",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MindeVieras/album-api/issues"
  },
  "homepage": "https://github.com/MindeVieras/album-api#readme",
  "dependencies": {
    "aspect-ratio": "^2.0.2",
    "aws-sdk": "^2.501.0",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-validation": "^1.0.2",
    "http-status-codes": "^1.3.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.4.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.5",
    "sequelize": "^5.10.3",
    "sequelize-cli": "^5.5.0",
    "superagent": "^5.1.0",
    "swagger-ui-express": "^4.0.7",
    "validator": "^11.1.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "colony": "0.0.7",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsdoc": "^15.7.2",
    "faker": "^4.1.0",
    "jest": "^24.8.0",
    "nodemon": "^1.18.6",
    "rimraf": "^2.6.2"
  }
}
