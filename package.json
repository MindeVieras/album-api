{
  "name": "album-api",
  "version": "1.0.0",
  "description": "AWS album API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src\"",
    "build": "rimraf dist && npx babel src -D -d dist",
    "start": "rimraf dist && npx babel src -D -d dist --no-comments && node dist/index.js",
    "clean": "rimraf dist colony node_modules",
    "colony": "rimraf colony && npx babel src -D -d dist && npx colony dist/index.js --scale 4 --no-modules && rimraf dist",
    "test": "eslint src"
  },
  "engines": {
    "node": "10.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MindeVieras/album-api.git"
  },
  "author": "Minde Vieras",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MindeVieras/album-api/issues"
  },
  "homepage": "https://github.com/MindeVieras/album-api#readme",
  "dependencies": {
    "aspect-ratio": "^2.0.2",
    "aws-sdk": "^2.358.0",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-validation": "^1.0.2",
    "http-status-codes": "^1.3.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.4.0",
    "mariadb": "^2.0.5",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "sequelize": "^5.8.9",
    "superagent": "^4.0.0",
    "validator": "^10.9.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "babel-eslint": "^10.0.1",
    "colony": "0.0.7",
    "eslint": "^5.9.0",
    "nodemon": "^1.18.6",
    "rimraf": "^2.6.2",
    "swagger-ui-express": "^4.0.6"
  }
}
