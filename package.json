{
  "name": "album-api",
  "version": "1.0.0",
  "description": "AWS album API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src\"",
    "build": "rimraf dist && npx babel src -D -d dist",
    "start": "rimraf dist && npx babel src -D -d dist --no-comments && node dist/index.js",
    "clean": "rimraf dist colony node_modules",
    "test": "eslint src"
  },
  "engines": {
    "node": "10.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MindeVieras/album-api.git"
  },
  "author": "Minde Vieras",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MindeVieras/album-api/issues"
  },
  "homepage": "https://github.com/MindeVieras/album-api#readme",
  "dependencies": {
    "@hapi/joi": "^16.1.7",
    "aspect-ratio": "^2.1.0",
    "aws-sdk": "^2.565.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "superagent": "^5.1.0",
    "swagger-ui-express": "^4.1.2",
    "validator": "^12.0.0",
    "winston": "^3.2.1",
    "winston-mysql": "^0.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.0",
    "@babel/node": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.6.0",
    "nodemon": "^1.19.4",
    "rimraf": "^3.0.0"
  }
}
