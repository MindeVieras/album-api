{
  "name": "album-api",
  "version": "1.0.0",
  "description": "AWS album API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "rimraf dist && tsc",
    "start": "node dist/index.js",
    "clean": "rimraf dist colony node_modules && npm install",
    "colony": "rimraf colony && tsc && npx colony dist/index.js --scale 4 --no-modules",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "test-migrate": "NODE_ENV=test npx sequelize-cli db:migrate",
    "test-seed": "NODE_ENV=test npx sequelize-cli db:seed:all",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "test": "NODE_ENV=test PORT=3003 nyc mocha"
  },
  "engines": {
    "node": "10.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MindeVieras/album-api.git"
  },
  "author": "Minde Vieras",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MindeVieras/album-api/issues"
  },
  "homepage": "https://github.com/MindeVieras/album-api#readme",
  "dependencies": {
    "aspect-ratio": "^2.0.2",
    "aws-sdk": "^2.502.0",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-validation": "^1.0.2",
    "http-status-codes": "^1.3.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.4.0",
    "moment": "^2.22.2",
    "mysql2": "^1.6.5",
    "sequelize": "^5.11.0",
    "sequelize-cli": "^5.5.0",
    "superagent": "^5.1.0",
    "swagger-ui-express": "^4.0.7",
    "validator": "^11.1.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/joi": "^14.3.3",
    "@types/node": "^12.6.8",
    "chai": "^4.2.0",
    "colony": "0.0.7",
    "faker": "^4.1.0",
    "mocha": "^6.2.0",
    "nodemon": "^1.18.6",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.2",
    "sqlite3": "^4.0.9",
    "supertest": "^4.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  }
}
