{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "completed-docs": [
      true,
      {
        "classes": true,
        "enums": true,
        "enum-members": true,
        "functions": true,
        "interfaces": true,
        "namespaces": true,
        "types": true,
        "variables": true
      }
    ],
    "no-console": false,
    "object-literal-sort-keys": false,
    "one-line": false,
    "ordered-imports": false,
    "semicolon": [true, "never"],
    "trailing-comma": false,
    "quotemark": [true, "single"]
  },
  "rulesDirectory": []
}
